{
  "expo": {
    "name": "crypto-app",
    "slug": "crypto-app",
    "version": "1.0.0",
    "scheme": "crypto-app",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "experiments": {
      "typesRoutes": true
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/app-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.pajlma.crypto",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["expo-router", "expo-notifications"],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "63964dbb-96ec-4448-ab28-7d7925422d18"
      }
    }
  }
}
